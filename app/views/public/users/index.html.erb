<h1>会員一覧</h1>
<%= render 'list',keyword: @keyword, tag_lists: @tag_lists %>

<% @users.each do |user| %>
<table>
  <tr>
    <td><%= image_tag user.get_profile_image(40,40) %></td>
    <td>
      <%= link_to user_path(user.id) do %>
        <%= user.nickname %>
      <%end%>
    </td>
    <td>　</td>
    <td><%= user.introduction %></td>
  </tr>
</table>
<%end%>